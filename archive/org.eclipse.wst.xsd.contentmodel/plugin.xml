<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.wst.xsd.contentmodel"
   name="Content Model for XML Schema"
   version="1.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.wst.xsd.contentmodel.internal.CMPluginXSD">

   <runtime>
      <library name="cmxsd.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime.compatibility"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.wst.contentmodel"/>
      <import plugin="org.eclipse.wst.encoding"/>
      <import plugin="org.eclipse.wst.uriresolver"/>
      <import plugin="org.eclipse.xsd"/>
   </requires>


   <extension
         point="org.eclipse.wst.contentmodel.documentFactories">
      <factory
            type="xsd"
            class="org.eclipse.wst.xsd.contentmodel.internal.CMDocumentFactoryXSD">
      </factory>
   </extension>

</plugin>
