<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.wst.sse.unittests"
   name="Unittests"
   version="1.0.0"
   provider-name="Eclipse.org">
   <runtime>
      <library name="ssejunit.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jst.jsp.core.tests"/>
      <import plugin="org.eclipse.jst.jsp.tests.encoding"/>
      <import plugin="org.eclipse.jst.jsp.ui.tests"/>
      <import plugin="org.eclipse.jst.jsp.ui.tests.performance"/>
      <import plugin="org.eclipse.wst.css.core.tests"/>
      <import plugin="org.eclipse.wst.css.tests.encoding"/>
      <import plugin="org.eclipse.wst.css.ui.tests.performance"/>
      <import plugin="org.eclipse.wst.dtd.ui.tests"/>
      <import plugin="org.eclipse.wst.html.core.tests"/>
      <import plugin="org.eclipse.wst.html.tests.encoding"/>
      <import plugin="org.eclipse.wst.html.ui.tests"/>
      <import plugin="org.eclipse.wst.html.ui.tests.performance"/>
      <import plugin="org.eclipse.wst.sse.core.tests"/>
      <import plugin="org.eclipse.wst.sse.ui.tests"/>
      <import plugin="org.eclipse.wst.sse.ui.tests.performance"/>
      <import plugin="org.eclipse.wst.xml.core.tests"/>
      <import plugin="org.eclipse.wst.xml.tests.encoding"/>
      <import plugin="org.eclipse.wst.xml.ui.tests"/>
      <import plugin="org.eclipse.wst.xml.ui.tests.performance"/>
      <import plugin="org.eclipse.wst.xml.validation.tests"/>
   </requires>

</plugin>
