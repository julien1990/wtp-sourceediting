<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.jst.jsp.ui.tests.performance"
   name="JSP UI Performance Plug-in"
   version="1.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.jst.jsp.ui.tests.performance.JSPUIPerformancePlugin">

   <runtime>
      <library name="jspuiperformance.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.jst.jsp.core"/>
      <import plugin="org.eclipse.wst.xml.core"/>
      <import plugin="org.eclipse.wst.sse.core"/>
      <import plugin="org.eclipse.wst.sse.ui"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.wst.common.encoding"/>
      <import plugin="org.eclipse.wst.html.ui"/>
      <import plugin="org.eclipse.wst.xml.ui"/>
      <import plugin="org.eclipse.wst.css.core"/>
      <import plugin="org.eclipse.wst.html.core"/>
      <import plugin="org.eclipse.wst.css.ui"/>
      <import plugin="org.eclipse.wst.javascript.common.ui"/>
      <import plugin="org.eclipse.jst.jsp.ui"/>
      <import plugin="org.eclipse.test.performance"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.swt"/>
   </requires>

</plugin>
