<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
	id="org.eclipse.wst.sse.core.tests"
	name="Tests Plug-in"
	version="1.0.0"
	provider-name="IBM"
	class="org.eclipse.wst.sse.core.tests.SSEModelTestsPlugin">

	<runtime>
		<library name="ssemodeltests.jar">
			<export name="*" />
		</library>
	</runtime>
	<requires>
		<import plugin="org.eclipse.core.runtime.compatibility" />
		<import plugin="org.eclipse.core.resources" />
		<import plugin="org.junit" />
  <import plugin="org.eclipse.wst.encoding"/>
  <import plugin="org.eclipse.wst.xml.tests.encoding"/>
  <import plugin="org.eclipse.wst.sse.core"/>
	</requires>

	<extension
		id="ssemodeltests"
		point="org.eclipse.core.runtime.applications">
		<application>
			<run
				class="org.eclipse.wst.sse.core.tests.application.SSEModelTests">
			</run>
		</application>
	</extension>

